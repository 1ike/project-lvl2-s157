{
  "name": "1ike/gendiff",
  "description": "Compare config pair",
  "version":"0.0.1",
  "license": "MIT",

  "bin": ["bin/gendiff"],

  "autoload": {
    "psr-4": {
      "Differ\\": "src",
      "Differ\\Tests\\": "tests"
    },
    "files": [
        "src/docopt.php",
        "src/Differ.php"
    ]
  },

  "require": {
    "docopt/docopt": "^1.0",
    "symfony/finder": "^3.3"
  },

  "scripts": {
    "phpcs": "phpcs",
    "phpcbf": "phpcbf",
    "phpunit": "phpunit"
  },

  "require-dev": {
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer":"*",
    "codeclimate/php-test-reporter": "dev-master"
  }
}
